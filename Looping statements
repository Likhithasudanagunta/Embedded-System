void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  int a,b,i;
  a=5,b=7;
 
  while(i<b){
    Serial.println(i);
    i++;
  }
  Serial.println("WHILE Loop Ends");
do{
  Serial.println(i);
  i++;
  }while(i<b);
  Serial.println("DO WHILE Loop Ends");
}

